/*组件*/
/*卡片*/
.fcard{
    position: relative;
}
.fcard:active{
	background-color: #f8f8f8;
	color: #666;
}
.fcard-item{
    position: relative;
    box-sizing: border-box;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.fcard-body-content,.fcard-content{
    width: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.fcard-body-content{
	margin: 20px 0;
}
.fcard-media{
    padding-right: 15px;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-shrink: 0;
    -ms-flex: 0 0 auto;
    -webkit-flex-shrink: 0;
    flex-shrink: 0;
    -webkit-box-lines: single;
    -moz-box-lines: single;
    -webkit-flex-wrap: nowrap;
    flex-wrap: nowrap;
    box-sizing: border-box;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.fcard-inner{
    position: relative;
    width: 100%;
    overflow: hidden;
    box-sizing: border-box;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-flex: 1;
    -ms-flex: 1;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.fcard-title{
    -webkit-flex-shrink: 1;
    -ms-flex: 0 1 auto;
    -webkit-flex-shrink: 1;
    flex-shrink: 1;
    max-width: 100%;		
}
.fcard-subtitle{
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.fcard-after{
    padding-right: 15px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-flex-shrink: 0;
    -ms-flex: 0 0 auto;
    -webkit-flex-shrink: 0;
    flex-shrink: 0;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}
.fcard:after{
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    right: auto;
    top: auto;
    height: 1px;
    width: 100%;
    background-color: #e5e5e5;
    display: block;
    z-index: 15;
    -webkit-transform-origin: 50% 100%;
    transform-origin: 50% 100%;
}
.fcard-header:after{
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    right: auto;
    top: auto;
    height: 1px;
    width: 100%;
    background-color: #eee;
    display: block;
    z-index: 15;
    -webkit-transform-origin: 50% 100%;
    transform-origin: 50% 100%;
}
.fcard-footer:before{
    content: '';
    position: absolute;
    left: 0;
    bottom: auto;
    right: auto;
    top: 0;
    width: 100%;
    height: 1px;
    background-color: #e5e5e5;
    display: block;
    z-index: 15;
    -webkit-transform-origin: 50% 0%;
    transform-origin: 50% 0%;
}
@media only screen and (-webkit-min-device-pixel-ratio:2){
	.fcard:after, .fcard-header:after, .fcard-footer:before {    
		-webkit-transform: scaleY(.5);
		transform: scaleY(.5);
	}
}
@media only screen and (-webkit-min-device-pixel-ratio:3){
	.fcard:after, .fcard-header:after, .fcard-footer:before{    
		-webkit-transform: scaleY(.33);
	    	transform: scaleY(.33);
	}
}

/*表格*/
.item-table table{
	width: 100%; 
	font-size: 14px;
	border-collapse: separate;
}
.item-table thead{
	background-color: #f8f8f8;
}
.item-table tbody{
	background-color: #fff;
}
.item-table th{
	font-weight: normal;
	color: #333;
}
.item-table td{
	color: #666;
}
.item-table th, .item-table td{
	width: 115px;
	padding: 15px 10px;
	font-size: 14px;
	line-height: 20px;
}
.item-table thead th{
	border-top: 1px solid #e5e5e5;
}
.item-table thead th:last-child, .item-table tr td:last-child{
	border-right: 1px solid #e5e5e5;
}
.item-table tr:last-child td{
	border-bottom: 1px solid #e5e5e5;
}
.item-table th:first-child, .item-table td:first-child{
	border-left: 1px solid #e5e5e5;
}
.item-table tbody td{
	border-top: 1px solid #eee;
}
.item-table thead th,.item-table tbody td{
	border-right: 1px solid #eee;
}
.item-table thead th:first-child{
	border-radius: 3px 0 0;
}
.item-table thead th:last-child{
	border-radius: 0 3px 0 0;
}
.item-table tbody tr:last-child td:first-child{
	border-radius: 0 0 0 3px;
}
.item-table tbody tr:last-child td:last-child{
	border-radius: 0 0 3px 0;
}

